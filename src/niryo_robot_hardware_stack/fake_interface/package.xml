  <exec_depend>niryo_robot_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>tools_interface</exec_depend>
  <exec_depend>joints_interface</exec_depend>
  <exec_depend>conveyor_interface</exec_depend>
